{
    "name": "jsdecena/payjunction",
    "description": "Payjunction package for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jsdecena\\Payjunction\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jsdecena\\Payjunction\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jeff Simons Decena",
            "email": "jeff.decena@yahoo.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jsdecena\\Payjunction\\PayjunctionProvider"
            ]
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0",
        "illuminate/validation": "^6.0|^7.0|^8.0",
        "illuminate/translation": "^6.0|^7.0|^8.0",
        "league/fractal": "^0.19.2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.17"
    }
}
