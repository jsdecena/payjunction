{
    "name": "jsdecena/payjunction",
    "description": "Payjunction package for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jsdecena\\Payjunction\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jsdecena\\Payjunction\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jeff Simons Decena",
            "email": "jeff.decena@yahoo.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jsdecena\\Payjunction\\PayjunctionProvider"
            ]
        }
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.19"
    }
}
